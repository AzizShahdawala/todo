<%- include("header") %>

    <div class="box" id="heading">
        <h1> <%= ListTitle %> </h1>
    </div>

    <div class="box">
        <% for(var i=0;i<ListItem.length;i++){ %>
        <div class="item">
            <input type="checkbox">
            <p> <%= ListItem[i] %> </p>
        </div>
        <% } %>

        <form class="item" action="/" method="POST">

            <input type="text" name="item" placeholder="List your item here" autocomplete="off">
            <button type="submit" name="list" value=" <%= ListTitle %>">+</button>

        </form>

    </div>

<%- include("footer") %>



